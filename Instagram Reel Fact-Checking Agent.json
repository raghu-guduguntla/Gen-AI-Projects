    {
      "parameters": {
        "jsCode": "const out = $input.first().json; // Format Final Report output\n\nconst esc = (s) => String(s ?? \"\")\n  .replaceAll(\"&\", \"&amp;\")\n  .replaceAll(\"<\", \"&lt;\")\n  .replaceAll(\">\", \"&gt;\")\n  .replaceAll('\"', \"&quot;\")\n  .replaceAll(\"'\", \"&#039;\");\n\nconst isUrl = (s) => /^https?:\\/\\/\\S+$/i.test(String(s ?? \"\").trim());\n\nconst asLink = (s) => {\n  const t = String(s ?? \"\").trim();\n  if (!t) return \"\";\n  if (!isUrl(t)) return `<span class=\"srcText\">${esc(t)}</span>`;\n  return `<a class=\"srcLink\" href=\"${esc(t)}\" target=\"_blank\" rel=\"noreferrer noopener\">${esc(t)}</a>`;\n};\n\nconst normalizeVerdict = (vRaw) => {\n  const v = String(vRaw ?? \"\").toUpperCase().trim();\n  if (v === \"TRUE\") return \"TRUE\";\n  if (v === \"FALSE\") return \"FALSE\";\n  if (v === \"PARTIALLY_TRUE\" || v === \"PARTIALLY TRUE\") return \"PARTIALLY_TRUE\";\n  if (v === \"UNVERIFIABLE\") return \"UNVERIFIABLE\";\n  // fallback\n  if (v.includes(\"PART\")) return \"PARTIALLY_TRUE\";\n  if (v.includes(\"UNVER\")) return \"UNVERIFIABLE\";\n  if (v.includes(\"FALS\")) return \"FALSE\";\n  if (v.includes(\"TRUE\")) return \"TRUE\";\n  return \"UNVERIFIABLE\";\n};\n\nconst verdictStyle = (v) => {\n  switch (v) {\n    case \"TRUE\":\n      return { label: \"TRUE\", fg: \"#166534\", bg: \"#dcfce7\", border: \"#22c55e\" };\n    case \"FALSE\":\n      return { label: \"FALSE\", fg: \"#991b1b\", bg: \"#fee2e2\", border: \"#ef4444\" };\n    case \"PARTIALLY_TRUE\":\n      return { label: \"PARTIALLY TRUE\", fg: \"#92400e\", bg: \"#fef3c7\", border: \"#f59e0b\" };\n    default:\n      return { label: \"UNVERIFIABLE\", fg: \"#374151\", bg: \"#e5e7eb\", border: \"#9ca3af\" };\n  }\n};\n\nconst claims = Array.isArray(out.claims) ? out.claims : [];\nconst total = claims.length;\nconst trueCount = claims.filter(c => normalizeVerdict(c.verdict) === \"TRUE\").length;\n\nconst claimCards = claims.map((c, i) => {\n  const v = normalizeVerdict(c.verdict);\n  const st = verdictStyle(v);\n\n  const why = String(c.reasoning ?? \"\").trim();\n  const whyHtml = `\n    <div class=\"why\">\n      <span class=\"label\">Why:</span>\n      <div class=\"whyText\">${esc(why || \"No explanation was provided for this claim.\")}</div>\n    </div>\n  `;\n\n  const sources = Array.isArray(c.sources) ? c.sources.filter(Boolean) : [];\n  const sourcesHtml = sources.length\n    ? `\n      <div class=\"sources\">\n        <span class=\"label\">Sources:</span>\n        <div class=\"srcList\">${sources.map(asLink).filter(Boolean).join(\" ‚Ä¢ \")}</div>\n      </div>\n    `\n    : `\n      <div class=\"sources\">\n        <span class=\"label\">Sources:</span>\n        <span class=\"srcText\">None provided</span>\n      </div>\n    `;\n\n  return `\n    <div class=\"claimCard\">\n      <div class=\"claimTop\">\n        <div class=\"claimNum\">#${i + 1}</div>\n        <div class=\"badge\" style=\"background:${st.bg};color:${st.fg};border-color:${st.border};\">\n          ${st.label}\n        </div>\n      </div>\n\n      <div class=\"claimText\">${esc(c.claim ?? \"\")}</div>\n      ${whyHtml}\n      ${sourcesHtml}\n    </div>\n  `;\n}).join(\"\");\n\nconst body = `<!doctype html>\n<html>\n<head>\n  <meta charset=\"utf-8\"/>\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\"/>\n  <title>Reel Authenticity Report</title>\n  <style>\n    body{\n      font-family: system-ui, -apple-system, Segoe UI, Roboto, Arial;\n      background:#f6f7fb; margin:0; padding:24px; color:#0f172a;\n    }\n    .wrap{max-width:920px;margin:0 auto;}\n    .header{\n      background:#ffffff; border:1px solid #e5e7eb; border-radius:16px;\n      padding:18px; box-shadow:0 10px 30px rgba(0,0,0,.06);\n    }\n    h1{margin:0 0 10px;font-size:28px;}\n    .scoreRow{display:flex;gap:12px;align-items:center;flex-wrap:wrap;}\n    .scoreBox{\n      display:flex; align-items:baseline; gap:10px;\n      padding:10px 12px; border-radius:12px; border:1px solid #e5e7eb;\n      background:#f8fafc;\n    }\n    .scoreNum{font-size:28px;font-weight:900;color:#2563eb;}\n    .scoreMeta{font-size:13px;color:#475569;font-weight:700;}\n    .subtitle{color:#64748b;margin:10px 0 0;font-size:13px;line-height:1.35;}\n    .legend{margin-top:12px;display:flex;gap:8px;flex-wrap:wrap}\n    .legend .pill{font-size:12px;font-weight:900;border-radius:999px;padding:6px 10px;border:1px solid #e5e7eb;background:#fff;color:#334155}\n    .claimsWrap{margin-top:14px;}\n    .claimCard{\n      background:#ffffff; border:1px solid #e5e7eb; border-radius:14px;\n      padding:14px; margin:12px 0; box-shadow:0 6px 18px rgba(0,0,0,.04);\n    }\n    .claimTop{display:flex;align-items:center;gap:10px;}\n    .claimNum{font-weight:900;color:#0f172a;background:#f1f5f9;border-radius:999px;padding:4px 10px;font-size:12px;}\n    .badge{\n      font-weight:900; font-size:12px; padding:6px 10px; border-radius:999px;\n      border:1px solid;\n    }\n    .claimText{margin-top:10px;font-size:15px;font-weight:800;line-height:1.35;}\n    .label{font-weight:900;color:#334155;}\n    .why, .sources{margin-top:10px;color:#334155;font-size:13px;line-height:1.4;}\n    .whyText{margin-top:6px;white-space:pre-wrap;}\n    .srcList{margin-top:6px;}\n    .srcLink{color:#2563eb;text-decoration:underline;word-break:break-all;}\n    .srcText{color:#334155;}\n    @media (max-width: 720px){\n      body{padding:14px;}\n      h1{font-size:22px;}\n      .scoreNum{font-size:22px;}\n    }\n  </style>\n</head>\n\n<body>\n  <div class=\"wrap\">\n    <div class=\"header\">\n      <h1>üé¨ Reel Authenticity Report</h1>\n\n      <div class=\"scoreRow\">\n        <div class=\"scoreBox\">\n          <div class=\"scoreNum\">${trueCount}</div>\n          <div class=\"scoreMeta\">\n            out of ${total} claimed facts are true\n          </div>\n        </div>\n      </div>\n\n      <div class=\"subtitle\">\n        Score counts only <b>TRUE</b> facts as correct. <b>FALSE</b>, <b>PARTIALLY TRUE</b>, and <b>UNVERIFIABLE</b> are not counted as correct.\n      </div>\n\n      <div class=\"legend\">\n        <span class=\"pill\">‚úÖ TRUE</span>\n        <span class=\"pill\">üü° PARTIALLY TRUE</span>\n        <span class=\"pill\">‚ùå FALSE</span>\n        <span class=\"pill\">‚ö™ UNVERIFIABLE</span>\n      </div>\n    </div>\n\n    <div class=\"claimsWrap\">\n      ${claimCards || `<div class=\"header\" style=\"margin-top:14px;\">No claims found.</div>`}\n    </div>\n  </div>\n</body>\n</html>`;\n\nreturn [{ ...out, body }];"
      },
